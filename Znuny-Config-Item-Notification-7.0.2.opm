    <CodeInstall Type="post">
#create general catalog with item
my $ItemID1 = $Kernel::OM-&gt;Get('Kernel::System::GeneralCatalog')-&gt;ItemAdd(
    Class         =&gt; 'ITSM::ConfigItem::AlertType',
    Name          =&gt; 'Email',
    ValidID       =&gt; 1,
    Comment       =&gt; 'Email',              
    UserID        =&gt; 1,
);
#create general catalog with item
my $ItemID2 = $Kernel::OM-&gt;Get('Kernel::System::GeneralCatalog')-&gt;ItemAdd(
    Class         =&gt; 'ITSM::ConfigItem::AlertType',
    Name          =&gt; 'Telegram',
    ValidID       =&gt; 1,
    Comment       =&gt; 'Telegram',              
    UserID        =&gt; 1,
);
</CodeInstall>
    <BuildDate>2023-08-18 04:55:08</BuildDate>
    <BuildHost>app.myserver.com</BuildHost>
    <Filelist>
        <File Location="Kernel/Config/Files/XML/ZZZCMDBAlertDateTime.xml" Permission="660" Encode="Base64"></File>
        <File Location="Custom/Kernel/System/ITSMConfigItem/Event/CMDBAlertDateTime.pm" Permission="660" Encode="Base64"></File>
        <File Location="var/webservices/examples/CI Telegram Notification.yml" Permission="660" Encode="Base64"></File>
    </Filelist>
</otrs_package>